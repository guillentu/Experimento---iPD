reference "table_matrixIPD_T3"

\end_inset

 con T = 3 y 
\begin_inset CommandInset ref
LatexCommand ref
reference "table_matrixIPD_T4"

\end_inset

 con T=4 que favorecen la no cooperación.
\end_layout

\end_inset


\end_layout


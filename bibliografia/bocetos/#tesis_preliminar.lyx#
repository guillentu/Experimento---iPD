\begin_layout Plain Layout
\align center

\size scriptsize
\begin_inset Graphics
	filename /home/guille/Documents/github_Repository/experimento/Experimento---iPD/ExtraerDatos/figura_iPD_1_2_9s_13s/fig_finales/mean_reward.eps
	lyxscale 70
	width 45text%

